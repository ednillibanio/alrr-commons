<?xml version="1.0"?>
<faces-config metadata-complete="false" version="2.2"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>pg-feriados</from-outcome>
			<to-view-id>/pages/commons/pg-feriados.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>pg-localidades</from-outcome>
			<to-view-id>/pages/commons/pg-localidades.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>pg-bairros</from-outcome>
			<to-view-id>/pages/commons/pg-bairros.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application>
		<resource-bundle>
			<base-name>br.leg.rr.al.core.componentes</base-name>
			<var>core</var>
		</resource-bundle>
	</application>
</faces-config>
